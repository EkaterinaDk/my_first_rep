Repka = [
["Дедка" ,  "1820" , "человек",  "изба", "репка"],
["Бабка", "1825",  "человек",  "изба", "колобок"],
["Внучка" , "1865", "человек", "изба", "пирожок"],
[ "Жучка" , "1875", "собака", "конура", "кость"],
[ "Мурка" , "1876", "кошка", "чердак",  "мышка"],
["Мышка" , "1877", "мышь", "подвал", "сыр"]   
]
q = "сыр"

def my_search (x):
    for row in Repka:
        for elem in row:
            if elem == x:
                print (row[0])
                
my_search(q)
